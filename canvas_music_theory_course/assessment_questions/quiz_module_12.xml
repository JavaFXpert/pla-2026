  <item ident="ic37c3e4ac385453ca22ee811a73139ae" title="Understanding Melody">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Melody?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>A sequence of pitches perceived as a single, coherent musical line; the horizontal aspect of music</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
