  <item ident="icff157b1285d40b0a6fe2a314b1a868e" title="Understanding Staff (Stave)">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Staff (Stave)?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>A set of five horizontal lines and four spaces used to notate music</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
