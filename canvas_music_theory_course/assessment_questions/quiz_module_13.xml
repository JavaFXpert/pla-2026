  <item ident="i66887505d97f4d9297953e12f54da09b" title="Understanding Dynamics (Musical Dynamics)">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Dynamics (Musical Dynamics)?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>Variations in loudness; indicated by symbols like p (piano/soft) and f (forte/loud)</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
