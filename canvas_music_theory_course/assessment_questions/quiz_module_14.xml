  <item ident="i0b21557f41ea4ce2985ea943a4623b52" title="Understanding Musical Form">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Musical Form?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>The overall structure or architecture of a piece of music</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
