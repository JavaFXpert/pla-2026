  <item ident="idf5ce19999044fd29652ba7a3cde0d7e" title="Understanding Interval">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Interval?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>The distance between two pitches, measured in steps and quality</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
  <item ident="i25c6c935d1a34fdea65c489cb407b0a8" title="Application: Interval">
    <presentation>
      <material>
        <mattext texttype="text/html">Explain in your own words: Understand intervals as the relationship between two pitches</mattext>
      </material>
      <response_str ident="response1" rcardinality="Single"/>
    </presentation>
  </item>
  <item ident="id475a26f7d394889a7ce556cbf128ce7" title="Understanding Octave">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Octave?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>The interval between one pitch and another with double or half its frequency; notes an octave apart share the same letter name</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
  <item ident="i3f7f0c8ecdb74ad687f74d6c8f393eee" title="Application: Octave">
    <presentation>
