  <item ident="i5560558a1cbf4f9d8e80be7727180583" title="Understanding Interval Quality">
    <presentation>
      <material>
        <mattext texttype="text/html">Which of the following best describes Interval Quality?</mattext>
      </material>
      <response_lid ident="response1" rcardinality="Single">
        <render_choice>
          <response_label ident="correct">
            <material>
              <mattext>The specific character of an interval (major, minor, perfect, augmented, diminished)</mattext>
            </material>
          </response_label>
          <response_label ident="distractor1">
            <material>
              <mattext>A musical term for tempo variations</mattext>
            </material>
          </response_label>
          <response_label ident="distractor2">
            <material>
              <mattext>A type of musical instrument</mattext>
            </material>
          </response_label>
          <response_label ident="distractor3">
            <material>
              <mattext>A performance technique</mattext>
            </material>
          </response_label>
        </render_choice>
      </response_lid>
    </presentation>
  </item>
